{
    "name": "liberu-genealogy/genealogy-laravel",
    "type": "project",
    "description": "The Genealogy Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "filament/filament": "4.x",
        "guzzlehttp/guzzle": "^7.8",
        "laravel/framework": "^12.0",
        "laravel/jetstream": "^5.0",
        "laravel/octane": "*",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.9",
        "liberu-genealogy/laravel-dna": "^2.0",
        "liberu-genealogy/laravel-gedcom": "^6.5",
        "livewire/livewire": "^3.5",
        "spatie/laravel-menu": "^4.0"
    },
    "require-dev": {
        "driftingly/rector-laravel": "^2.0",
        "fakerphp/faker": "^2.0",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^12.1",
        "rector/rector": "*"
    },
"repositories": [
    {
      "url": "https://github.com/filamentphp/filament.git",
      "type": "git"
    }
],
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
  "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "12.x-dev"
        },
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
